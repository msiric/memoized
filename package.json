{
  "name": "memoized",
  "version": "1.0.0",
  "description": "A modern, open-source learning platform for programming education",
  "author": "Mario Siric <mario.siric@mail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/msiric/memoized.git"
  },
  "homepage": "https://memoized.io",
  "keywords": [
    "education",
    "programming",
    "typescript",
    "javascript",
    "data-structures",
    "algorithms",
    "learning-platform",
    "nextjs"
  ],
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next dev",
    "analyze": "cross-env NODE_OPTIONS=--max-old-space-size=8192 ANALYZE=true next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "build:dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 SENTRY_DISABLE=true next build",
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "migrate:dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "generate": "prisma generate",
    "sync": "cross-env tsx src/scripts/sync-content.ts",
    "sync:dev": "cross-env tsx --env-file .env src/scripts/sync-content.ts",
    "sync:clean": "cross-env tsx --env-file .env src/scripts/clean-database.ts && yarn sync:all:dev",
    "sync:resources": "cross-env tsx src/scripts/sync-resources.ts",
    "sync:resources:dev": "cross-env tsx --env-file .env src/scripts/sync-resources.ts",
    "sync:all": "yarn sync && yarn sync:resources",
    "sync:all:dev": "yarn sync:dev && yarn sync:resources:dev",
    "clean": "cross-env tsx --env-file .env src/scripts/clean-database.ts",
    "index:dev": "cross-env tsx --env-file .env src/scripts/index-search.ts",
    "index": "cross-env tsx src/scripts/index-search.ts",
    "webhook": "stripe listen --forward-to localhost:3000/api/webhook",
    "test": "vitest",
    "test:check": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:setup": "vitest run src/scripts/setup-dev.test.ts",
    "prebuild": "yarn run migrate",
    "postinstall": "yarn run generate",
    "install:clean": "cross-env tsx src/scripts/clean-dependencies.ts && yarn install",
    "setup:content": "cross-env tsx src/scripts/setup-content.ts",
    "setup:dev": "cross-env tsx src/scripts/setup-dev.ts"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@headlessui/tailwindcss": "^0.2.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/bundle-analyzer": "^15.3.1",
    "@next/mdx": "^14.0.4",
    "@next/third-parties": "^14.2.5",
    "@prisma/client": "^5.22.0",
    "@sentry/nextjs": "^8",
    "@sindresorhus/slugify": "^2.1.1",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "acorn": "^8.8.1",
    "autoprefixer": "^10.4.7",
    "clsx": "^2.1.0",
    "critters": "^0.0.25",
    "date-fns": "^3.6.0",
    "estree-walker": "2.0.2",
    "fast-glob": "^3.3.0",
    "framer-motion": "^10.18.0",
    "mdast-util-to-string": "^4.0.0",
    "mdx-annotations": "^0.1.1",
    "meilisearch": "^0.41.0",
    "next": "^14.2.3",
    "next-auth": "^4.24.7",
    "next-mdx-remote-client": "^2.1.3",
    "next-themes": "^0.2.1",
    "nextjs-toploader": "^1.6.12",
    "notistack": "^3.0.1",
    "prisma": "^5.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-icons": "^5.2.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.0.0",
    "resend": "^3.4.0",
    "shiki": "^1.24.4",
    "shiki-magic-move": "^0.5.0",
    "slugify": "^1.6.6",
    "stripe": "^15.8.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "unist-util-visit": "^5.0.0",
    "web-vitals": "^5.1.0",
    "zustand": "^4.5.2"
  },
  "resolutions": {
    "estree-walker": "2.0.2"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "@testing-library/dom": "^10.3.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/glob": "^8.1.0",
    "@types/mdx": "^2.0.8",
    "@types/node": "^20.10.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-highlight-words": "^0.20.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "2.1.8",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.4",
    "ollama": "^0.5.13",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sharp": "0.33.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "vitest": "^2.1.8"
  }
}
